
{
  "version": 2,
  "builds": [{
    "src": "api/proxy.js",
    "use": "@vercel/node"
  }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE"
      }
    },
    {
      "src": "/(.*)",
      "dest": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "X-Forwarded-Proto": "https",
        "Cache-Control": "no-cache"
      }
    }
  ],
  "regions": ["hkg1"]
}
