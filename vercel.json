
{
  "version": 2,
  "builds": [
    { "src": "api/proxy.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "https://www.luogu.com/_next/static/$1",
      "headers": {
        "Host": "www.luogu.com",
        "X-Forwarded-Region": "hkg1"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/(.*)",
      "dest": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "X-Forwarded-For": "203.198.7.16"
      }
    }
  ],
  "regions": ["hkg1"],
  "redirects": [
    { "source": "/", "destination": "/problemset", "permanent": false }
  ]
}
