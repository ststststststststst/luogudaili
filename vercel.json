{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "Referer": "https://www.luogu.com/",
        "X-Forwarded-For": "$remote_addr",
        "CF-Connecting-IP": "$remote_addr"
      }
    }
  ],
  "regions": ["sfo1"], 
  "rewrites": [
    { "source": "/cdn-cgi/(.*)", "destination": "https://www.luogu.com/cdn-cgi/$1" }
  ]
}
"functions": {
  "api/*.php": { "runtime": "nodejs18.x" }
}
