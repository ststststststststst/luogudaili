{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "Referer": "https://www.luogu.com/",
        "X-Forwarded-For": "$remote_addr",
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      }
    }
  ],
  "regions": ["hkg1"]  // 选择香港节点（离日本近，延迟低）
}
