
{
  "version": 2,
  "builds": [
    { "src": "api/proxy.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    {
      "source": "/_next/static/(.*)",
      "destination": "https://www.luogu.com/_next/static/$1",
      "headers": {
        "Host": "www.luogu.com",
        "X-Forwarded-Region": "hkg1"
      }
    },
    {
      "source": "/api/:path*",
      "destination": "/api/proxy?path=:path*"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "X-Forwarded-For": "203.198.7.16"
      }
    }
  ],
  "regions": ["hkg1"],
  "redirects": [
    { 
      "source": "/", 
      "destination": "/problemset", 
      "permanent": false 
    }
  ]
}
