{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://www.luogu.com/$1",
      "headers": {
        "Host": "www.luogu.com",
        "Referer": "https://www.luogu.com/",
        "X-Forwarded-For": "$remote_addr",
        "CF-Connecting-IP": "$remote_addr",
        "Accept-Language": "en-US,en;q=0.9"
      }
    }
  ],
  "regions": ["sfo1"],  
  "rewrites": [
    { 
      "source": "/cdn-cgi/(.*)", 
      "destination": "https://www.luogu.com/cdn-cgi/$1" 
    }
  ]
}
